{
  "version": "1.0.0",
  "version_code": 1,
  "name": "Sudoku",
  "package_name": "game.sudoku.com",
  "description": "Game Description",
  "launch_path": "/index.html",
  "icons": {    
	"56": "/assets/icons/logo.png",
    "128": "/assets/icons/logo.png"
  },
  "developer": {
    "name": "JioGames Studios",
    "url": "https://jiogames.com"
  },
  "default_locale": "en",
  "fullscreen": true,
  "permissions":{
	"systemXHR": {
		"description": "Req by SSO, This permission is require to call web api"
	},
	"device-storage:sdcard": {
		"description": "Req by SSO readwrite readwrite access",
		"name": "device-storage:sdcard",
		"access": "readwrite"
	},
	"browser": {
		"description": "Required for embedding a browser iframe"
	},
	"serviceworker": {
		"description": "Required for push"
	},
	"desktop-notification": {
		"description": "Required for desktop notifications"
	},
	"volumemanager": {
		"description": "Required for volume control using native method"
	},
	"push": {
		"description": "Required for notifications"
	},
	"audio-capture": {
		"description": "Required to capture audio using getUserMedia()"
	},
	"datacall":{
		"description": "Required to check internet connection"
	},
	"cross-domain-content": ["https://domain-name/"]
  }
}
